{
  "name": "@drozdik.m/web",
  "version": "1.0.0",
  "description": "My personal website",
  "main": "dist/Module.js",
  "typings": "dist/Module.d.ts",
  "scripts": {
    "compile-scripts": "tsc && webpack",
    "watch-scripts": "tsc && webpack --watch",
    "compile-styles": "sass --load-path=node_modules --load-path=../ wwwroot/Sitemap:wwwroot/_dist/wwwroot/Sitemap wwwroot/Web:wwwroot/_dist/wwwroot/Web --style compressed",
    "watch-styles": "sass --load-path=node_modules --load-path=../ --watch wwwroot/Sitemap:wwwroot/_dist/wwwroot/Sitemap wwwroot/Web:wwwroot/_dist/wwwroot/Web --style compressed",
    "clear": "rmdir /S /Q \"wwwroot/_dist\"",
    "compile": "npm run compile-scripts && npm run compile-styles",
    "build": "npm update && npm update -D && npm run compile",
    "test": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/drozdik-m/martin-drozdik-web"
  },
  "keywords": [],
  "author": "Martin Drozd√≠k <ja@martin-drozdik.cz> (http://martin-drozdik.cz)",
  "license": "MIT",
  "dependencies": {
    "@drozdik.m/common-mixins": "^1.0.5",
    "@drozdik.m/google-analytics": "^1.0.1",
    "@drozdik.m/px-in-rem": "^1.0.1",
    "@drozdik.m/slow-scroll": "^1.1.0",
    "@drozdik.m/window-events": "^1.1.2"
  },
  "devDependencies": {
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0"
  }
}
